	"cmake.configureArgs": ["-DCMAKE_TOOLCHAIN_FILE=OFF"],
	"C_Cpp.default.compilerPath": "usr/bin/gcc",
	"editor.tokenColorCustomizations": {
		"[Darcula]": {
			"comments": "#9259098f",
			"numbers": "#0066ff",
			"strings": "#00ff37",
			"types": "#ffe600",
			"functions": "#9c0505",
			"textMateRules": [
				{
					"scope": "variable.other.local", "settings": { "foreground": "#cfcfcf", },
				},
				{
					"scope": "entity.name.namespace", "settings": { "foreground": "#74008e", }
				},
				{
					"scope": "variable.parameter", "settings": { "foreground": "#ffffff", },
				},
				{
					"scope": "variable.other.property", "settings": { "foreground": "#85beff", },
				},
				{
					"scope": "variable.global", "settings": { "foreground": "#ffffff", "fontStyle": "italic bold"},
				},
				{
					"scope": "property.static", "settings": { "foreground": "#ffffff", "fontStyle": "italic bold"},
				},
				{
					"scope": "method.static", "settings": { "foreground": "#eb0000", "fontStyle": "bold"},
				},
				{
					"scope": "macro", "settings": { "foreground": "#0040ff", "fontStyle": "italic bold"},
				},
				{
					"scope": "templateFunction", "settings": { "foreground": "#ffe600", },
				},
			]
		},
	}
}